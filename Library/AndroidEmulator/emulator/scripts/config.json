{
    "commands": {
        "is_created": {
            "name": "check emulator",
            "command": "app.sh",
            "arguments": [
                "is_created"
            ],
            "async": false
        },
        "start": {
            "name": "start emulator",
            "command": "app.sh",
            "arguments": [
                "start"
            ],
            "async": true
        },
        "create": [
            {
                "name": "stop emulator",
                "command": "app.sh",
                "arguments": [
                    "stop"
                ],
                "async": false
            },
            {
                "name": "create emulator",
                "command": "app.sh",
                "arguments": [
                    "create"
                ],
                "async": false
            },
            {
                "name": "start emulator",
                "command": "app.sh",
                "arguments": [
                    "start"
                ],
                "async": true
            },
            {
                "name": "wait emulator booted",
                "command": "app.sh",
                "arguments": [
                    "wait_booted"
                ],
                "async": false
            },
            {
                "name": "set emulator proxy",
                "command": "app.sh",
                "arguments": [
                    "set_proxy",
                    "{{HTTP_PROXY}}"
                ],
                "async": false
            },
            {
                "name": "install root ca into emulator",
                "command": "app.sh",
                "arguments": [
                    "install_ca",
                    "{{CA_FILE}}"
                ],
                "async": false
            },
            {
                "name": "install gapps",
                "command": "app.sh",
                "arguments": [
                    "install_gapps"
                ],
                "async": false
            },
            {
                "name": "stop emulator",
                "command": "app.sh",
                "arguments": [
                    "stop"
                ],
                "async": false
